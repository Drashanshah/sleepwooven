{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    <strong>{{name}}</strong>\n                    {{else}}\n                    {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/navigation":"<!--<nav class=\"navUser\">\n    {{> components/common/currency-selector}}\n</nav>-->\n\n<div class=\"header-top\">\n    <div class=\"containerC d-flex\">\n        <div class=\"site-brand\">\n            {{> components/common/store-logo}}\n        </div>\n        <div>\n            <div class=\"site-slogan\">\n                <span class=\"slogan-icon icons\"></span>\n                <h4 class=\"slogan-title\">Free Shipping. 100 Night Trial</h4>\n            </div>\n        </div>\n        <div class=\"header-btns-block d-flex\">\n            <div class=\"search-box\">\n                <button class=\"btns search-btn icons\"></button>\n            </div>\n            <div id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n                {{> components/common/quick-search}}\n            </div>\n\n            {{#if settings.gift_certificates_enabled}}\n            <div class=\"gift-box\">\n                <a href=\"{{urls.gift_certificate.purchase}}\">\n                    <button class=\"btns-icon gift-btn icons\"></button>\n                </a>\n            </div>\n            {{/if}}\n            <div class=\"signin-box\">\n                {{#if customer}}\n                <button class=\"grey-btn signin-btn user_account\">\n                    <a href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a> <span\n                        class=\"icons user-icon\"></span>\n                </button>\n                <button class=\"grey-btn signin-btn\">\n                    <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a> <span\n                        class=\"icons user-icon\"></span>\n                </button>\n                {{else}}\n                <button class=\"grey-btn signin-btn\">\n                    <a href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a><span class=\"icons user-icon\"></span>\n                </button>\n                {{/if}}\n            </div>\n            <div class=\"basket-box\">\n                <a\n                        class=\"navUser-action\"\n                        data-cart-preview\n                        data-dropdown=\"cart-preview-dropdown\"\n                        data-options=\"align:right\"\n                        href=\"{{urls.cart}}\">\n                    <span class=\"navUser-item-cartLabel\"><button class=\"btns-icon basket-btn icons\"></button></span>\n                    <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n                </a>\n\n                <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content\n                     aria-hidden=\"true\"></div>\n            </div>\n            <!--<div class=\"lang-box\">\n                <button class=\"btns-icon lang-btn icons\"></button>\n            </div>-->\n            <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            {{#if customer.store_credit.value '>' 0}}\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\">\n                    <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            {{/if}}\n        </div>\n    </div>\n</div>\n","components/common/quick-search":"<div class=\"search_quick\">\n    {{{ snippet \"forms_search\"}}}\n    <form action=\"{{urls.search}}\">\n            <div>\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"}
