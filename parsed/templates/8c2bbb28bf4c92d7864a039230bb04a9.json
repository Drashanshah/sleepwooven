{"components/common/subscription-form":"<h3 class=\"footer-lists--heading\">{{lang 'newsletter.subscribe'}}</h3>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n<form action=\"{{urls.subscribe.action}}\" method=\"post\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"subscribe-form\">\n            <input class=\"form-fieldC\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n            <button class=\"subscribe-btn btns-icon\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\"></button>\n        </div>\n</form>\n"}
