{"components/common/footer":"{{#if banners.bottom}}\n<div class=\"banners\" data-banner-location=\"bottom\">\n    <div class=\"banner\">\n        {{{limit banners.bottom 1}}}\n    </div>\n</div>\n{{/if}}\n\n<footer class=\"site-footer footer\" role=\"contentinfo\">\n    <div class=\"containerC\">\n        <div class=\"top-footer d-flex\">\n            <div class=\"footer-lists\">\n                <a href=\"{{urls.home}}\">\n                    <img src=\"{{cdn 'img/img/logo-wt.png'}}\" alt=\"Sleep Woover\" class=\"img-responsive\">\n                </a>\n            </div>\n            <ul class=\"footer-lists\">\n                <li class=\"footer-lists--heading\">About Us</li>\n                <li>\n                    <a href=\"/our-design-story\">Our Design Story</a>\n                </li>\n                <li>\n                    <a href=\"/shipping-returns/\">Shipping & Returns</a>\n                </li>\n                <li>\n                    <a href=\"/blog/\">Blog</a>\n                </li>\n                <li>\n                    <a href=\"/contact-us/\">Contact us </a>\n                </li>\n            </ul>\n            <ul class=\"footer-lists\">\n                <li class=\"footer-lists--heading\">{{lang 'footer.categories'}}</li>\n                {{#each categories}}\n                <li>\n                    <a href=\"{{url}}\">{{name}}</a>\n                </li>\n                {{/each}}\n            </ul>\n            <div class=\"footer-lists footer-subscribe-form\">\n                {{#if settings.show_newsletter_box}}\n                {{> components/common/subscription-form}}\n                {{/if}}\n            </div>\n            <ul class=\"footer-lists footer-social-link\">\n                <li>\n                    <a href=\"https://www.facebook.com/sleepwooven/\" target=\"_blank\">\n                        <span class=\"social-icon facebook\"></span>\n                        Facebook\n                    </a>\n                </li>\n                <li>\n                    <a href=\"https://www.instagram.com/sleepwooven/\" target=\"_blank\">\n                        <span class=\"social-icon instagram\"></span>\n                        Instagram\n                    </a>\n                </li>\n                <li>\n                    <a href=\"https://www.linkedin.com/company/sleepwooven/\" target=\"_blank\">\n                        <span class=\"social-icon linkedIn\"></span>\n                        LinkedIn\n                    </a>\n                </li>\n                <!--<li>\n                    <a href=\"#\">\n                        <span class=\"social-icon youtube\"></span>\n                        Youtube\n                    </a>\n                </li>-->\n            </ul>\n        </div>\n        <div class=\"bottom-footer\">\n            <div class=\"card-box\">\n                <!--{{> components/common/payment-icons}}-->\n                <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name=www.sleepwooven.com&amp;size=M&amp;lang=en\"></script>\n               <!--<a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n                    <img src=\"{{cdn 'img/img/secured-by.png'}}\" alt=\"secured\" class=\"img-responsive\">\n                </a>-->\n\n                <img src=\"{{cdn 'img/img/visa.png'}}\" alt=\"visa\" class=\"img-responsive\">\n                <img src=\"{{cdn 'img/img/master-card.png'}}\" alt=\"master card\" class=\"img-responsive\">\n                <img src=\"{{cdn 'img/img/american-express.png'}}\" alt=\"american express\" class=\"img-responsive\">\n                <img src=\"{{cdn 'img/img/paypal.png'}}\" alt=\"paypal\" class=\"img-responsive\">\n            </div>\n            {{#if theme_settings.show_copyright_footer}}\n            <ul class=\"bottom-footer-list\">\n                <li>